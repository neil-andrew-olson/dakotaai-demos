{
  "version": 2,
  "builds": [
    {
      "src": "apps/image-classifier/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/image-classifier/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "apps/image-classifier/pages/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
